<?xml version="1.0" encoding="UTF-8"?>
<beans
    xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="hydrogen" class="com.javatpoint.Hydrogen" factory-method="getHydrogen">
        <property name="period" value="1" />
        <property name="density" value="0.00008988" />
        <property name="boilingPoint" value="-252.87" />
        <property name="quantity" value="10" />
    </bean>

</beans>
